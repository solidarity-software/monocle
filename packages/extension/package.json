  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/vscode": "^1.76.0",
    "@vscode/test-electron": "^2.3.0",
    "glob": "^9.2.1",
    "mocha": "^10.2.0"
  }
}
