{
  "name": "@monocle-lang/extension",
  "displayName": "monocle-lang-ext",
  "description": "Official monocle lang extension",
  "version": "0.0.1",
  "publisher": "Solidarity Software, Inc",
  "repository": "https://github.com/solidarity-software/monocle.git",
  "engines": {
    "vscode": "^1.75.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./lib/extension.js",
